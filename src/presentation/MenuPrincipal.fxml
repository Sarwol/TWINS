<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.MenuPrincipalController">
    <stylesheets>
        <URL value="@/css/menuprincipal.css" />
    </stylesheets>
   <children>
      <Button fx:id="cartaButton" layoutX="28.0" layoutY="250.0" mnemonicParsing="false" onAction="#abrirPorCarta" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="nivelesButton" layoutX="28.0" layoutY="307.0" mnemonicParsing="false" onAction="#abrirNiveles" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="barajasButton" layoutX="427.0" layoutY="307.0" mnemonicParsing="false" onAction="#abrirBarajas" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="salirButton" layoutX="223.0" layoutY="348.0" mnemonicParsing="false" onAction="#salir" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="estandarButton" layoutX="223.0" layoutY="250.0" mnemonicParsing="false" onAction="#abrirEstandar" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="categoriaButton" layoutX="427.0" layoutY="250.0" mnemonicParsing="false" onAction="#abrirPorCategoria" prefHeight="38.0" prefWidth="154.0" />
      <Button fx:id="parametrosButton" disable="true" layoutX="223.0" layoutY="300.0" mnemonicParsing="false" onAction="#abrirParametros" prefHeight="38.0" prefWidth="154.0" />
      <ImageView fx:id="muteView" fitHeight="38.0" fitWidth="38.0" layoutX="543.0" layoutY="21.0" onMouseClicked="#changeMusic" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/appImages/muteOnIcon.png" />
         </image></ImageView>
      <ImageView fitHeight="38.0" fitWidth="38.0" layoutX="28.0" layoutY="21.0" onMouseClicked="#abrirParametros" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/appImages/config.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
